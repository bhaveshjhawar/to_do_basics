<%-include("header")-%>


    <div class="box" id="heading">
        <h1><%= pageTitle %></h1>
    </div>


    <div class="box">

        <% whichTask.forEach((e)=>{ %>
        <form action="/delete" method="post">

        <div class="item">

        <input type="checkbox" name="checkBox" value="<%= e._id %>" onchange="this.form.submit()" id="">
        <p> <%= e.name %> </p>
        
        </div>
        <input type="hidden" name="listName" value="<%= pageTitle %>">

        </form>
        <% }) %>
        

        <form action="/" method="post" class="item">
            <input type="text" name="newTask" placeholder="New Task" autocomplete="off">
            <button type="submit" name="List" value="<%= pageTitle %>">+</button>
        </form>
    </div>

<%-include("footer")-%>
   